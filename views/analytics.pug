extends layout

block content
    div(class="analytics-page")
        h1 Analytics
        table
            thead 
                tr
                    th(class="text-left") Path
                    th Visits
            tbody
                tr
                    td
                    td
                each pageView in pageViews
                    tr
                        td= pageView._id
                        td(class="text-center")= pageView.visits